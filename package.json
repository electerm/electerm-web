  "files": [
    "npm",
    "README.md",
    "LICENSE"
  ],
  "standard": {
    "globals": [
      "log",
      "MouseEvent",
      "WebSocket",
      "FileReader",
      "CustomEvent",
      "onmessage",
      "self"
    ],
    "ignore": [
      "/public/",
      "src/client/entry-web/rle.js"
    ],
    "sourceType": "module"
  }
}
