    "diffie-hellman": "^5.0.3",
    "dotenv": "16.3.1",
    "electerm-sync": "1.1.0",
    "electron-log": "^4.4.8",
    "eventemitter3": "5.0.1",
    "express": "4.18.2",
    "express-jwt": "^8.4.1",
