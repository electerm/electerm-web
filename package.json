    "ssh-config": "^4.4.1",
    "ssh2": "1.14.0",
    "strip-ansi": "^7.1.0",
    "stylus": "0.59.0",
    "tar": "6.1.15"
  },
  "files": [
    "npm",
    "README.md",
    "LICENSE"
  ],
  "standard": {
    "globals": [
      "log",
      "MouseEvent",
      "WebSocket",
      "FileReader",
      "CustomEvent",
      "onmessage",
      "self"
    ],
    "ignore": [
      "/public/"
    ],
    "sourceType": "module"
  }
}
